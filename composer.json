{
  "name": "gwl12345/modern-auth",
  "version": "1.0.2",
  "description": "A modern authentication package for Laravel 12 + Inertia 2.3",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "User",
      "email": "user@example.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "illuminate/contracts": "^12.0",
    "illuminate/support": "^12.0",
    "inertiajs/inertia-laravel": "^2.0",
    "laragear/webauthn": "^4.0",
    "jenssegers/agent": "^2.6"
  },
  "autoload": {
    "psr-4": {
      "Gwl12345\\ModernAuth\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Gwl12345\\ModernAuth\\ModernAuthServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
